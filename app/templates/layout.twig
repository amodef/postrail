<html lang="fr">
	<head>
		<title>Postrail 2016 | {% block title %}{% endblock %}</title>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1">

		<link rel="stylesheet" href="{{ base_url() }}/css/main.css">
		<!--[if lte IE 9]><link rel="stylesheet" href="{{ base_url() }}/css/ie9.css" /><![endif]-->
	</head>
	<body>

		<!-- Page Wrapper -->
		<div id="page-wrapper">

			<!-- Header -->
			{% include 'header.twig' %}
			
			<!-- Wrapper -->
			<section id="wrapper">
				<header>
					<div class="inner">
						<h2>Postrail</h2>
						<p>Samedi 23 juillet 2016</p>
					</div>
				</header>

				<!-- Content -->
				<div class="wrapper">
    				<div class="inner">
						{% block content %}{% endblock %}
					</div>
				</div>

			</section>

			<!-- Footer -->
			<section id="footer">
				<div class="inner">
					{% block footer %}
					<ul class="copyright">
						<li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
					</ul>
					{% endblock %}
				</div>
			</section>

		</div>

		<!-- Scripts -->
		<script src="{{ base_url() }}/js/skel.min.js"></script>
		<script src="{{ base_url() }}/js/jquery.min.js"></script>
		<script src="{{ base_url() }}/js/jquery.scrollex.min.js"></script>
		<script src="{{ base_url() }}/js/util.js"></script>
		<script src="{{ base_url() }}/js/sorttable.js"></script>
		<script src="{{ base_url() }}/js/main.js"></script>
		{% block script %}{% endblock %}
	</body>
</html>